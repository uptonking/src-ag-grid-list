{
  "name": "sample-grid-vanilla-js",
  "version": "23.2.1",
  "description": "sample vanilla app for ag-grid",
  "keywords": [
    "datagrid"
  ],
  "license": "MIT",
  "main": "src/index",
  "types": "dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "src/**/*.js",
    "dist",
    "README.md",
    "scripts/"
  ],
  "scripts": {
    "build-babel": "babel src --out-dir dist --config-file ./babel.config.js --extensions \".ts,.js\"  --source-maps inline",
    "build-demo": "webpack --config ./config/webpack.config.prod.js",
    "build-es": "yarn run clean && yarn run build-tsc",
    "build-tsc": "tsc -p ./tsconfig.json",
    "build-types": "tsc --emitDeclarationOnly",
    "build1": "yarn run build:types && npm run build:js",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "clean": "rimraf -rf dist",
    "demo": "webpack serve --hot --config ./webpack/webpack.config.demo.js",
    "demo-vanilla": "webpack-cli serve --hot --config ./config/webpack.config.vanilla.js",
    "formatter": "prettier --config ./.prettierrc.js --loglevel log --write '**/*.{ts,tsx,js,jsx,json,css}'",
    "lint": "eslint src --ext .ts,.tsx --fix -o eslint-log.log",
    "start": "yarn run demo",
    "storybook": "start-storybook -p 8999 -c .storybook",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-+++++++++++++++++ -- --watch"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "23.2.1",
    "@ag-grid-community/core": "23.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/plugin-proposal-decorators": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-react": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@webpack-cli/serve": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "7.14.0",
    "eslint-config-alloy": "^3.8.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "polished": "^3.6.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "source-map-loader": "^1.1.2",
    "source-map-support": "^0.5.19",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "ts-jest": "^25.4.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
