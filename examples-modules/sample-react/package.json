{
  "name": "sample-grid-react",
  "version": "23.2.1",
  "description": "sample react app for ag-grid",
  "keywords": [
    "datagrid"
  ],
  "license": "MIT",
  "main": "dist/main.js",
  "files": [
    "CHANGELOG.md",
    "dist",
    "README.md",
    "scripts/"
  ],
  "scripts": {
    "build-1": "npm run build:types && npm run build:js",
    "build-es": "yarn run clean && webpack --config ./config/webpack.config.demo.js",
    "build-ts": "babel src --out-dir lib --watch",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf -rf dist",
    "demo": "webpack-cli serve --hot --config ./config/webpack.config.demo.js",
    "demo-vanilla": "webpack-cli serve --hot --config ./config/webpack.config.vanilla.js",
    "lint": "eslint src --ext .ts,.tsx --fix -o eslint-log.log",
    "prettier": "prettier --config ./.prettierrc.js --loglevel log --write '**/*.{ts,tsx,js,jsx,json,css}'",
    "start": "yarn run demo",
    "storybook": "start-storybook -p 8999 -c .storybook",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "23.2.1",
    "@ag-grid-community/core": "23.2.1",
    "ag-grid-react": "23.2.1",
    "sample-grid-vanilla-js": "23.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@webpack-cli/serve": "^1.0.1-alpha.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-alloy": "^3.7.4",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "polished": "^3.6.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-refresh": "^0.9.0-rc.0",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^1.0.2",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "ts-jest": "^25.4.0",
    "typescript": "~3.6.5",
    "webpack": "^5.0.0-beta.28",
    "webpack-cli": "^4.0.0-beta.8",
    "webpack-dev-server": "^3.11.0",
    "yarn": "^1.22.4"
  }
}
