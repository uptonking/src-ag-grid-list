{
  "name": "sample-grid-react",
  "version": "23.2.1",
  "description": "sample react app for ag-grid",
  "keywords": [
    "datagrid"
  ],
  "license": "MIT",
  "main": "dist/main.js",
  "files": [
    "CHANGELOG.md",
    "dist",
    "README.md",
    "scripts/"
  ],
  "scripts": {
    "build-1": "npm run build:types && npm run build:js",
    "build-es": "npm run clean && webpack --config ./config/webpack.config.demo.js",
    "build-ts": "babel src --out-dir lib --watch",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf -rf dist",
    "demo": "webpack-cli serve --hot --config ./config/webpack.config.demo.js",
    "demo-vanilla": "webpack-cli serve --hot --config ./config/webpack.config.vanilla.js",
    "lint": "eslint src --ext .ts,.tsx --fix -o eslint-log.log",
    "formatter": "prettier --config ./.prettierrc.js --loglevel log --write '**/*.{ts,tsx,js,jsx,json,css}'",
    "start": "npm run demo",
    "storybook": "start-storybook -p 8999 -c .storybook",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "23.2.1",
    "@ag-grid-community/core": "23.2.1",
    "@ag-grid-community/react": "23.2.1",
    "sample-grid-vanilla": "23.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/plugin-proposal-decorators": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-react": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@webpack-cli/serve": "^2.0.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "7.14.0",
    "eslint-config-alloy": "^3.8.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "polished": "^3.6.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^5.0.1",
    "sass-loader": "^13.3.1",
    "source-map-loader": "^4.0.1",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.3",
    "styled-components": "^5.2.1",
    "ts-jest": "^25.4.0",
    "typescript": "^4.9.0",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
