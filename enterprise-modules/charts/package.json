  "description": "Advanced Data Grid / Data Table supporting Javascript / React / AngularJS / Web Components",
  "main": "./dist/cjs/main.js",
  "module": "./dist/es6/main.js",
  "types": "./dist/cjs/main.d.ts",
  "scripts": {
    "test": "jest",
    "build-cjs": "tsc -p tsconfig.json",
