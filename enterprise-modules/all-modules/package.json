{
  "name": "@ag-grid-enterprise/all-modules",
  "version": "23.2.1",
  "description": "Advanced Data Grid / Data Table supporting Javascript / React / AngularJS / Web Components",
  "keywords": [
    "enterprise",
    "ag",
    "ag-grid",
    "datagrid",
    "data-grid",
    "datatable",
    "data-table",
    "grid",
    "table",
    "react",
    "angular",
    "angular-component",
    "react-component",
    "angularjs",
    "reactjs"
  ],
  "homepage": "http://www.ag-grid.com/",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "license": "Commercial",
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "main": "./dist/cjs/main.js",
  "module": "./dist/es6/main.js",
  "types": "./dist/cjs/main.d.ts",
  "scripts": {
    "build": "npm run precheck && npm run lint && npm run build-cjs && npm run build-es6 && gulp copy-grid-core-styles && npm run hash",
    "build-cjs": "tsc -p tsconfig.json",
    "build-css": "npx gulp copy-grid-core-styles",
    "build-es6": "tsc -p tsconfig.es6.json",
    "clean": "rimraf dist",
    "e2e": "protractor protractor.conf.js",
    "e2e:debug": " node --inspect-brk ./node_modules/.bin/protractor protractor.conf.js",
    "hash": "sh ../../scripts/hashDirectory.sh > .hash",
    "lint": "tsc -p tsconfig-lint.json && tslint --project tsconfig.json",
    "package": "npm run webpack-updater && npx gulp webpack-all-no-clean && npm run rollup",
    "precheck": "tsc -p tsconfig-precheck.json",
    "rollup": "node ../../module-build/rollup/build.js --bundlePrefix ag-grid-enterprise",
    "start-server": "http-server .",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --no-cache -runInBand",
    "testWithCoverage": "jest --coverage",
    "update-webdriver": "webdriver-manager update --gecko false",
    "validate-es5": "eslint dist/lib",
    "watchWebpack": "webpack --watch",
    "webdriver-kill": "kill $(ps -ef | grep webdriver | grep -v grep | awk '{ print $2 }')",
    "webdriver-start": "npm run update-webdriver && webdriver-manager start --standalone --detach",
    "webpack-updater": "node webpackUpdater.js",
    "xtest:e2ex": "start-server-and-test start-server http://localhost:8080 e2e"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 11",
    "not blackberry > 0"
  ],
  "dependencies": {
    "@ag-grid-community/all-modules": "23.2.1",
    "@ag-grid-enterprise/charts": "23.2.1",
    "@ag-grid-enterprise/clipboard": "23.2.1",
    "@ag-grid-enterprise/column-tool-panel": "23.2.1",
    "@ag-grid-enterprise/core": "23.2.1",
    "@ag-grid-enterprise/date-time-cell-editor": "23.2.1",
    "@ag-grid-enterprise/excel-export": "23.2.1",
    "@ag-grid-enterprise/filter-tool-panel": "23.2.1",
    "@ag-grid-enterprise/master-detail": "23.2.1",
    "@ag-grid-enterprise/menu": "23.2.1",
    "@ag-grid-enterprise/range-selection": "23.2.1",
    "@ag-grid-enterprise/rich-select": "23.2.1",
    "@ag-grid-enterprise/row-grouping": "23.2.1",
    "@ag-grid-enterprise/server-side-row-model": "23.2.1",
    "@ag-grid-enterprise/set-filter": "23.2.1",
    "@ag-grid-enterprise/side-bar": "23.2.1",
    "@ag-grid-enterprise/status-bar": "23.2.1",
    "@ag-grid-enterprise/viewport-row-model": "23.2.1"
  },
  "devDependencies": {
    "@ag-grid-community/core": "23.2.1",
    "@types/jest": "^26.0.16",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.0",
    "eslint": "7.14.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-ie11": "^1.0.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-header": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-tslint": "^8.1.0",
    "gulp-typescript": "^5.0.1",
    "http-server": "^0.11.1",
    "jest": "^26.6.3",
    "merge2": "1.4.1",
    "postcss-loader": "^3.0.0",
    "protractor": "^5.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "start-server-and-test": "^1.10.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-jest": "^25.4.0",
    "tslint": "^5.19.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-stream": "5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
