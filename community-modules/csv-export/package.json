{
  "name": "@ag-grid-community/csv-export",
  "version": "23.2.1",
  "description": "Advanced Data Grid / Data Table supporting Javascript / React / AngularJS / Web Components",
  "keywords": [
    "web-components",
    "grid",
    "data",
    "table",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "angularjs",
    "reactjs"
  ],
  "homepage": "http://www.ag-grid.com/",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "license": "MIT",
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "main": "./dist/cjs/main.js",
  "module": "./dist/es6/main.js",
  "types": "./dist/es6/main.d.ts",
  "scripts": {
    "build-1": "npm run build-cjs && npm run build-es6 && npm run hash",
    "build-cjs": "tsc -p tsconfig.json",
    "build-es": "tsc -p tsconfig.es6.json",
    "build-es6": "tsc -p tsconfig.es6.json",
    "clean": "rimraf -rf dist",
    "hash": "sh ../../scripts/hashDirectory.sh > .hash",
    "package": "node ../../module-build/rollup/build.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 11",
    "not blackberry > 0"
  ],
  "dependencies": {
    "@ag-grid-community/core": "23.2.1"
  },
  "devDependencies": {
    "typescript": "~3.6.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
